{
  "name": "billysbilling",
    "type": "library",
    "description": "PHP-SDK to communicate with BillysBilling",
    "homepage": "http://github.com/lsolesen/billysbilling",
    "authors": [
        {
            "name": "Lars Olesen",
            "email": "lars@intraface.dk"
        },
        {
          "name": "Matt Glaman",
          "email": "nmd.matt@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BillysBilling\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "BillysBilling\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=5.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "dev-master"
    }
}
